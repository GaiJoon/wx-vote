.item-container {
  display: flex;
  flex-direction: column;
}

.item-item {
  display: flex;
  width: 750rpx;
  height: 202rpx;
  /* border: 1px solid red; */
}

.item-image {
  width: 100rpx;
  height: 100rpx;
  margin-left: 60rpx;
  margin-top: 50rpx;
  border-radius: 20rpx;
}

.item-log {
  width: 30rpx;
  height: 40rpx;
  float: left;
  margin-top: 5px;
}

.item-log1 {
  width: 30rpx;
  height: 40rpx;
  float: left;
  margin-top: 5px;

}

.item-log2 {
  width: 30rpx;
  height: 40rpx;
  float: left;
  margin-top: 5px;

}
/* .item-container:nth-child(1) .item-log,
{
  display: inline-block;
} */
.item-group {
  float: right;
}

.item-name {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333;
  letter-spacing: -0.34px;
}

.item-ticket {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999;
  letter-spacing: -0.29px;
  display:block;
  float:left;
  width:100rpx; 
  margin-left: 6rpx;
  margin-top: 10rpx;
  margin-right:5rpx;
}

.item-percentage {
  display:block;
  width:60rpx;
  font-family:PingFangSC-Regular;
  font-size:12px;
  color:#999;
  letter-spacing:-0.43px;
  text-align:right;
  float:left;
  margin-top:10rpx;

}

.item-title {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999;
  letter-spacing: -0.29px;
  text-align: center;
  float: left;
  /* margin-top: 20px; */
}

.item-slogan {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999;
  letter-spacing: -0.29px;
  text-align: right;
  float: right;
}

.item-progressBar {
  width: 260px;
  float: left;
  margin-top: 8rpx;
  border-radius: 100px;
  overflow: hidden;
}

.item-group-two {
  margin-left: 10rpx;
  margin-top: 40rpx;
  /* border: 1px solid blueviolet; */
}

.item-group-one {
  /* border: 1px solid blueviolet; */
  margin-bottom: 8rpx;
}

.item-line {
  width: 100%;
  height: 1px;
  background: #d8d8d8;
}
