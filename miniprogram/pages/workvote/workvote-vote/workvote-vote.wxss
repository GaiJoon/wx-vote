.vo-container {
  display: flex;
  flex-direction: column;
}

.vo-content {
  width: 698rpx;
  height: 1028rpx;
  margin: 0 auto;
}

.vo-title {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #030303;
  letter-spacing: -0.34px;
  margin-top: 20rpx;
  margin-left: 30rpx;
}

.vo-title-detail {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999;
  letter-spacing: -0.29px;
  margin-left: 20rpx;
}

.vo-introduce {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #030303;
  letter-spacing: -0.34px;
  text-align: center;
  margin-left: 30rpx;
  display: block;
  float: left;
}

.vo-introduce-detail {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999;
  letter-spacing: -0.29px;
  margin-left: 20rpx;
  display: block;
  width: 558rpx;
  float: left;
}

.divLine {
  background: #e0e3da;
  width: 100%;
  height: 1rpx;
  margin-top: 18rpx;
}

.vo-group {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #030303;
  letter-spacing: -0.34px;
  margin-top: 19rpx;
  margin-left: 30rpx;
}

.vo-member-img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.vo-groups {
  width: 558rpx;
  margin: 0 auto;
  margin-bottom: 120rpx;
}

.vo-member-name {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #030303;
  letter-spacing: -0.34px;
  text-align: center;
  margin-left: 20rpx;
}

.vo-ren {
  float: left;
  margin-left: 100rpx;
  margin-top: 32rpx;
  display: flex;
  align-items: center;
}

.vo-ren:nth-child(2n+1) {
  margin-left: 0;
}

.vo-vote {
  background: #333;
  width: 554rpx;
  height: 96rpx;
  border-radius: 6px;
  color: #fff;
  font-family: PingFangSC-Regular;
  font-size: 18px;
  letter-spacing: -0.43px;
  text-align: center;
  line-height: 48px;
  margin-left: 60rpx;
  margin-top: 14rpx;
}

.vo-votes {
  background:#A2A6A6;
  width: 277px;
  height: 48px;
  border-radius: 6px;
  color: #fff;
  font-family: PingFangSC-Regular;
  font-size: 18px;
  letter-spacing: -0.43px;
  text-align: center;
  line-height: 48px;
  margin-left: 60rpx;
  margin-top: 14rpx;
}

.vo-vf {
  width: 100vw;
  height: 64px;
  background: #fff;
  margin-top: 1.6vw;
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
}

.vo-forwords {
  float:right;
  line-height:1;
  background-color:transparent;
  padding:0;
  margin-right: 40rpx;
  margin-top: 40rpx;
}
.vo-forwords::after {
  border: 0;
}
.vo-forword {
  width:48rpx;
  height: 42rpx;
}

